{
  "name": "matt.travi.org",
  "description": "my profile",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "private": true,
  "repository": "travi-org/matt.travi.org",
  "bugs": "https://github.com/travi-org/matt.travi.org/issues",
  "homepage": "https://travi-org.github.io/matt.travi.org",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "sign:security": "gpg --armor --output static/.well-known/security.txt.sig --detach-sig static/.well-known/security.txt",
    "signature:security": "gpg --verify static/.well-known/security.txt.sig static/.well-known/security.txt",
    "test": "run-s lint:* signature:* build",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gh-pages --silent -r \"https://$GH_TOKEN@github.com/travi-org/matt.travi.org.git\" -d public/",
    "start": "run-s develop",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.7.1",
    "@travi/eslint-config-travi": "1.6.6",
    "commitlint-config-travi": "1.2.23",
    "cz-conventional-changelog": "2.1.0",
    "gh-pages": "1.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.13.3",
    "husky": "0.15.0-rc.3",
    "markdownlint-cli": "0.7.1",
    "npm-run-all": "4.1.2"
  },
  "dependencies": {
    "@travi/matt.travi.org-components": "1.1.0",
    "gatsby": "1.9.175",
    "gatsby-link": "1.6.35",
    "gatsby-plugin-google-fonts": "0.0.3",
    "gatsby-plugin-react-helmet": "2.0.4",
    "gatsby-plugin-react-next": "1.0.8",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0"
  }
}
