{
  "name": "matt.travi.org",
  "description": "my profile",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "private": true,
  "repository": "travi-org/matt.travi.org",
  "bugs": "https://github.com/travi-org/matt.travi.org/issues",
  "homepage": "https://travi-org.github.io/matt.travi.org",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:md": "remark . --frail",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "sign:security": "gpg --armor --output static/.well-known/security.txt.sig --detach-sig static/.well-known/security.txt",
    "signature:security": "gpg --verify static/.well-known/security.txt.sig static/.well-known/security.txt",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:* --serial signature:* build",
    "test:e2e": "CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./public --prod",
    "packtracker:publish": "packtracker-upload --stats=public/stats.json",
    "start": "run-s develop",
    "commitmsg": "commitlint -e",
    "precommit": "npm test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@packtracker/webpack-plugin": "2.1.2",
    "@travi/any": "2.0.2",
    "@travi/eslint-config-travi": "1.8.2",
    "ban-sensitive-files": "1.9.2",
    "commitlint-config-travi": "1.3.1",
    "cypress": "3.4.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint-plugin-cypress": "2.6.0",
    "gatsby-cypress": "0.1.9",
    "husky": "3.0.1",
    "netlify-cli": "2.11.24",
    "npm-run-all": "4.1.5",
    "remark-cli": "6.0.1",
    "start-server-and-test": "1.9.1",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "@material-ui/core": "4.0.1",
    "@material-ui/styles": "4.0.1",
    "@travi/matt.travi.org-components": "3.0.6",
    "gatsby": "2.7.5",
    "gatsby-link": "2.1.1",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "2.0.20",
    "gatsby-plugin-google-fonts": "1.0.0",
    "gatsby-plugin-material-ui": "2.0.1",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-webmention": "0.1.0",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  }
}
